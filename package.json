{
  "name": "big-eyes-token",
  "version": "1.0.0",
  "description": "Big Eyes Token",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm install --save-dev hardhat-deploy-ethers && rm package-lock.json && node scripts/patchUniswapLibrary.mjs",
    "hardhat": "node --require esm node_modules/.bin/hardhat",
    "flatten": "run(){ yarn hardhat flatten contracts/$1.sol > Flat$1.sol; }; run",
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test",
    "mempool": " node src/getContractInternalTransactions.js",
    "coverage": "yarn hardhat clean && yarn hardhat coverage",
    "deploy": "yarn hardhat deploy --network",
    "verify": "yarn hardhat etherscan-verify --network",
    "redeploy": "func() { rm -rf deployments/\"$1\" && yarn hardhat deploy --network \"$1\"; }; func",
    "coveralls": "yarn coverage && cat coverage/lcov.info | coveralls",
    "ganache:start": "ganache-cli",
    "solhint": "solhint 'contracts/**/*.sol'",
    "solhint:fix": "solhint --fix 'contracts/**/*.sol'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "gas-report": "env ENABLE_GAS_REPORT=1 npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-solhint": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@openzeppelin/hardhat-defender": "^1.7.0",
    "@openzeppelin/hardhat-upgrades": "^1.19.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@rauschma/stringio": "^1.4.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "^1.7.0",
    "bn.js": "^4.11.0",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "esm": "^3.2.25",
    "ethereum-waffle": "^3.4.4",
    "ethers": "~5.5.0",
    "hardhat": "^2.9.9",
    "hardhat-deploy": "^0.11.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "replace-in-file": "^6.3.5",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.21",
    "typescript": "^4.7.4",
    "web3": "^1.7.4"
  }
}
